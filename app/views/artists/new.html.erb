<h1> Create Artist <h1>

<% if flash[:artist_errors]%>
<% flash[:artist_errors].each do |e| %>
  <p> <%= e %> <p>
  <% end %>
  <% end %>

  <%= form_for @artist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.submit %>
    <%end%>
  


